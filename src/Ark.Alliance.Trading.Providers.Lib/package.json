{
  "name": "ark-alliance-trading-providers-lib",
  "version": "1.1.0",
  "description": "Multi-provider TypeScript library for cryptocurrency exchange APIs (Binance, Deribit, Kraken)",
  "author": "Armand Richelet-Kleinberg <armand@m2h.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArmandRicheletKleinberg/Ark.Alliance.Trading.Providers.Lib.git"
  },
  "homepage": "https://github.com/ArmandRicheletKleinberg/Ark.Alliance.Trading.Providers.Lib#readme",
  "bugs": {
    "url": "https://github.com/ArmandRicheletKleinberg/Ark.Alliance.Trading.Providers.Lib/issues"
  },
  "keywords": [
    "binance",
    "deribit",
    "kraken",
    "crypto",
    "trading",
    "futures",
    "websocket",
    "typescript"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./Binance": {
      "import": "./dist/Binance/index.js",
      "types": "./dist/Binance/index.d.ts"
    },
    "./Deribit": {
      "import": "./dist/Deribit/index.js",
      "types": "./dist/Deribit/index.d.ts"
    },
    "./Kraken": {
      "import": "./dist/Kraken/index.js",
      "types": "./dist/Kraken/index.d.ts"
    },
    "./Common/services": {
      "import": "./dist/Common/services/index.js",
      "types": "./dist/Common/services/index.d.ts"
    },
    "./Services": {
      "import": "./dist/Services/index.js",
      "types": "./dist/Services/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/express-rate-limit": "^5.1.3",
    "@types/node": "^25.0.3",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "socket.io": "^4.8.3",
    "typescript": "^5.9.3",
    "winston": "^3.19.0"
  },
  "dependencies": {
    "uuid": "^13.0.0",
    "ws": "^8.16.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}