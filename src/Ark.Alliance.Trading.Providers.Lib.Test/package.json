    "author": "Ark Alliance",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^29.5.12",
        "@types/node": "^20.10.0",
        "@types/uuid": "^9.0.7",
        "@types/ws": "^8.5.10",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "ark-alliance-trading-providers-lib": "file:../Ark.Alliance.Trading.Providers.Lib",
        "uuid": "^9.0.1",
        "ws": "^8.16.0"
    }
}