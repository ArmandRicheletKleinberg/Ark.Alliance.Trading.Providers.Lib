{
    "name": "ark-alliance-trading-providers-lib-test",
    "version": "1.0.0",
    "description": "Comprehensive test suite for Ark Alliance Trading Providers Library",
    "main": "dist/index.js",
    "scripts": {
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:binance": "jest --testPathPattern=Binance",
        "test:integration": "jest --testPathPattern=Integration",
        "test:scenarios": "jest --testPathPattern=scenarios",
        "test:live": "ts-node Src/Scripts/RunLiveTests.ts",
        "test:live:account": "ts-node Src/Scripts/RunLiveTests.ts ACCOUNT",
        "test:live:orders": "ts-node Src/Scripts/RunLiveTests.ts ORDERS",
        "test:live:positions": "ts-node Src/Scripts/RunLiveTests.ts POSITIONS",
        "test:live:market": "ts-node Src/Scripts/RunLiveTests.ts MARKET_DATA",
        "test:live:rate-limit": "ts-node Src/Scripts/RunLiveTests.ts RATE_LIMIT",
        "test:live:streaming": "ts-node Src/Scripts/RunLiveTests.ts STREAMING",
        "test:execute": "ts-node Src/Scripts/ExecuteLiveTests.ts",
        "test:execute:account": "ts-node Src/Scripts/ExecuteLiveTests.ts ACCOUNT",
        "test:execute:market": "ts-node Src/Scripts/ExecuteLiveTests.ts MARKET_DATA",
        "test:verbose": "ts-node Src/Scripts/VerboseTestExecution.ts",
        "lint": "eslint . --ext .ts",
        "build": "tsc"
